<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonView">
        <attr name="src" format="reference"/>
        <attr name="title" format="string"/>
        <attr name="desc" format="string"/>
    </declare-styleable>

    <declare-styleable name="VideoButton">
        <attr name="startColor" format="color"/>
        <attr name="endColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="AlphaButton">
        <attr name="normalAlpha" format="float" />
        <attr name="pressedAlpha" format="float" />
        <attr name="disableAlpha" format="float" />
    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="activeLineColor" format="color"/>
        <attr name="inactiveLineColor" format="color"/>
        <attr name="circleColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="circleRadius" format="dimension"/>
        <attr name="delayShowText" format="integer"/>
        <attr name="textPadding" format="dimension"/>
        <attr name="animationTime" format="integer"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textHeight" format="dimension"/>
        <attr name="progress" format="float"/>
        <attr name="showProgressText" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="PropertyTextView">
        <attr name="propertyTitle" format="string"/>
        <attr name="propertyValue" format="string"/>
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="backWidth" format="dimension" />    <!--背景圆环宽度-->
        <attr name="progWidth" format="dimension" />    <!--进度圆环宽度-->
        <attr name="backColor" format="color" />        <!--背景圆环颜色-->
        <attr name="progColor" format="color" />        <!--进度圆环颜色-->
        <attr name="progStartColor" format="color" />   <!--进度圆环开始颜色-->
        <attr name="progFirstColor" format="color" />   <!--进度圆环结束颜色-->
        <attr name="progress1" format="integer" />       <!--圆环进度-->
    </declare-styleable>
</resources>





