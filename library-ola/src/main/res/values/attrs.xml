    <declare-styleable name="BannerView">
        <attr name="banner_page_margin" format="dimension" />
        <attr name="banner_auto_scroll" format="boolean"/>
        <attr name="banner_toggle_duration" format="integer"/>
        <attr name="banner_radius" format="dimension" />
    </declare-styleable>


    <declare-styleable name="ScaleSlideBar">
        <attr name="slide_paddingLeft" format="dimension|reference" />
        <attr name="slide_paddingTop" format="dimension|reference" />
        <attr name="slide_paddingRight" format="dimension|reference" />
        <attr name="slide_paddingBottom" format="dimension|reference" />
        <attr name="slide_background" format="color|reference" />
        <attr name="slide_anchor_height" format="dimension|reference" />
        <attr name="slide_anchor_width" format="dimension|reference" />
        <attr name="slide_placeholder_height" format="dimension|reference" />
        <!--slide_placeholder_width:背景坑位的半径-->
        <attr name="slide_placeholder_width" format="dimension|reference" />
        <attr name="slide_type" format="integer|reference" />
        <attr name="slide_textSize" format="dimension|reference" />
        <attr name="slide_textColor" format="color|reference" />
        <attr name="slide_text_margin" format="dimension|reference" />

    </declare-styleable>

    <declare-styleable name="CropSeekBar">
        <attr name="audio_slide_out_h" format="dimension" />
        <attr name="audio_radio" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ScrollTrackView">
        <attr name="background_color" format="color"/>
        <attr name="foreground_color" format="color"/>
        <attr name="space_size" format="dimension"/>
        <attr name="track_item_width" format="dimension"/>
        <attr name="track_fragment_count" format="integer"/>
        <attr name="auto_run" format="boolean"/>
        <attr name="cut_duration" format="integer"/>
        <attr name="loop_run" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CenterMaskView">
        <attr name="civLineColor" format="color" />
        <attr name="civLineWidth" format="dimension" />
        <attr name="civMaskColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="backWidth" format="dimension" />    <!--背景圆环宽度-->
        <attr name="progWidth" format="dimension" />    <!--进度圆环宽度-->
        <attr name="backColor" format="color" />        <!--背景圆环颜色-->
        <attr name="progColor" format="color" />        <!--进度圆环颜色-->
        <attr name="progStartColor" format="color" />   <!--进度圆环开始颜色-->
        <attr name="progFirstColor" format="color" />   <!--进度圆环结束颜色-->
        <attr name="progress1" format="integer" />       <!--圆环进度-->
    </declare-styleable>

</resources>